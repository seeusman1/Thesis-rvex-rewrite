+-- txa(dsutx, 16#00#, 16#00#, 16#00#, 16#12#, txp);
+--
+-- txc(dsutx, 16#c0#, txp);
+-- txa(dsutx, 16#90#, 16#40#, 16#00#, 16#60#, txp);
+-- txa(dsutx, 16#00#, 16#00#, 16#12#, 16#10#, txp);
