+-- txc(dsutx, 16#c0#, txp);
+-- txa(dsutx, 16#90#, 16#00#, 16#00#, 16#20#, txp);
+-- txa(dsutx, 16#00#, 16#00#, 16#ff#, 16#ff#, txp);
