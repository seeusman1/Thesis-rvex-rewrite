+      txc(dsutx, 16#55#, txp);          -- sync uart
+      txc(dsutx, 16#a0#, txp);
+      txa(dsutx, 16#40#, 16#00#, 16#00#, 16#00#, txp);
+      rxi(dsurx, w32, txp, lresp);
