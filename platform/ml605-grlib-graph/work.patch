+     apbo(6) <= apb_none;
+     lcd_datal <= (others => '0'); clkvga_p <= '0'; clkvga_n <= '0';
+     lcd_hsyncl <= '0'; lcd_vsyncl <= '0'; lcd_del <= '0';
+     dvi_i2co.scloen <= '1'; dvi_i2co.sdaoen <= '1';
+  end generate;
