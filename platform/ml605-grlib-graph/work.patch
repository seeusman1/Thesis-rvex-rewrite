+proc readhelp {tag fn}  {
+	set message ""
+	set b 0
+	if { [file readable $fn] == 1} then {
+		set fhandle [open $fn r]
+		while {[gets $fhandle inline] >= 0} {
+			if { $b == 0 } {
+				if { [regexp $tag $inline ] } {
+					set b 1
+					set message "$inline:\n"
+				}
+			} else {
+				if { [regexp {^[^ \t]} $inline]} {
+					break
+				}
+				set message "$message\n$inline"
+			}
+		}
+		close $fhandle
+	}
+	return $message
+}
