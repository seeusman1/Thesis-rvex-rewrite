name    Test general purpose register file with forwarding

-- Check configuration.
config  genBundleSize         8
config  bundleAlign           2
config  memLaneRevIndex       1
config  forwarding            1

-- Initialize memories.
init

-- 0x000
load    add r0.1 = r0.0, 42
load    mtl l0.0 = r0.0 ;;

-- 0x008
load    stl 0[r0.0] = l0.0 -- Should be 0, still
load    mtl l0.0 = r0.1 ;;

-- 0x010
load    stl 0[r0.0] = l0.0 -- Should be 42 here.
load    add r0.1 = r0.0, 123 ;;

-- 0x018
load    stl 0[r0.0] = l0.0 -- Should still be 42 here.
load    nop ;;

-- 0x020
load    mtl l0.0 = r0.1
load    nop ;;

-- 0x028
load    stl 0[r0.0] = l0.0 -- Should be 123 here.
load    nop ;;

-- 0x030
load    stl 0[r0.0] = l0.0 -- Should still be 123 here.
load    nop ;;

-- 0x038
load    stl 0[r0.0] = l0.0 -- Should still be 123 here.
load    nop ;;

-- 0x040
load    stl 0[r0.0] = l0.0 -- Should still be 123 here.
load    nop ;;

-- 0x048
load    stl 0[r0.0] = l0.0 -- Should still be 123 here.
load    nop ;;

-- 0x050
load    stl 0[r0.0] = l0.0 -- Should still be 123 here.
load    stop ;;

-- Run the test.
reset
wait    40 write * 0   0 exclusive
wait    40 write * 0  42 exclusive
wait    40 write * 0  42 exclusive
wait    40 write * 0 123 exclusive
wait    40 write * 0 123 exclusive
wait    40 write * 0 123 exclusive
wait    40 write * 0 123 exclusive
wait    40 write * 0 123 exclusive
wait    40 write * 0 123 exclusive
wait    40 idle 0

