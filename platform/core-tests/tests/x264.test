name    x264 kernel test

-- Check configuration.
config  genBundleSize         8
config  memLaneRevIndex       1
config  branchLaneRevIndex    0
config  forwarding            1
config  limmhFromPreviousPair 1
config  limmhFromNeighbor     1

-- Initialize memories.
init

-- Load srec file into data and instruction memories.
srec    imem 0 srec/x264.srec
srec    dmem 0 srec/x264.srec

-- Run the program.
reset
wait    1000    write * 0x33333330 0
wait    100000  write * 0x33333340 1920
wait    1000    write * 0x33333344 1920
wait    1000    write * 0x33333348 1920
wait    1000    write * 0x3333334C 1920
